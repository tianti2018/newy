<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../config/ehcache.xsd">  
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="10000" overflowToDisk="true"
		eternal="false" memoryStoreEvictionPolicy="LRU" maxElementsOnDisk="10000000"
		diskExpiryThreadIntervalSeconds="0" timeToIdleSeconds="0"
		timeToLiveSeconds="100000" diskPersistent="false" />
	<cache name="dictionaryCache" maxElementsInMemory="10000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="0" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="urlRoleCache" maxElementsInMemory="10000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="0" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="menuCache" maxElementsInMemory="10000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="0" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="simCommonCache" maxElementsInMemory="10000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="0" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
</ehcache>